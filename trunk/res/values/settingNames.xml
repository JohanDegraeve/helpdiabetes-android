<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
    This file is needed to get the required values from the setting table in the database
    
    Every string in here can be found in the database settings.name
    -->

    <!-- Insuline ratio setting names -->
    <string name="setting_insuline_ratio_breakfast">insulineRatioBreakfast</string>
    <string name="setting_insuline_ratio_lunch">insulineRatioLunch</string>
    <string name="setting_insuline_ratio_dinner">insulineRatioDinner</string>
    <string name="setting_insuline_ratio_snack">insulineRatioSnack</string>

    <!-- Meal time setting names -->
    <string name="setting_meal_time_breakfast">mealTimeBreakfast</string>
    <string name="setting_meal_time_lunch">mealTimeLunch</string>
    <string name="setting_meal_time_snack">mealTimeSnack</string>
    <string name="setting_meal_time_dinner">mealTimeDinner</string>

    <!-- Font size settings -->
    <string name="setting_font_size">fontSize</string>

    <!-- Language -->
    <string name="setting_language">language</string>

    <!-- meal unit -->
    <string name="setting_glucose_unit">glucoseUnit</string>
    
    <!-- value on/off settings -->
    <string name="setting_value_carb_onoff">valueCarbOnOff</string>
    <string name="setting_value_prot_onoff">valueProtOnOff</string>
    <string name="setting_value_fat_onoff">valueFatOnOff</string>
    <string name="setting_value_kcal_onoff">valueKcalOnOff</string>
    
    <!-- value default -->
    <!-- 1 = carb, 2 = prot, 3 = fat, 4 = kcal -->
    <string name="setting_value_default">valueDefault</string>
    
    <!-- Default medicine type (used to add selected food to tracking page) -->
    <string name="setting_default_medicine_type_ID">defaultMedicineTypeID</string>
    
    <!-- To check if application just started -->
    <string name="setting_startUp">startUp</string>
    
</resources>